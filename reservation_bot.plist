<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
    <dict>
        <key>Label</key>
        <string>reservation_bot</string>

        <key>ProgramArguments</key>
        <array>
            <string>/usr/bin/python3</string>
            <string>/usr/local/bin/reservation_bot/bot.py</string>
        </array>

        <key>RunAtLoad</key>
        <false/>

        <key>StartCalendarInterval</key>
        <dict>
            <key>Weekday</key>
            <integer>4</integer>

            <key>Hour</key>
            <integer>8</integer>

            <key>Minute</key>
            <integer>0</integer>
        </dict>

    </dict>
</plist>

